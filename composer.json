{
    "name": "wuzhihui819/phpinfo",
    "description": "laravel-admin phpinfo extension",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/wuzhihui819/phpinfo",
    "license": "MIT",
    "authors": [
        {
            "name": "wuzhihui819",
            "email": "wuzhihui819@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "~1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\PHPInfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\PHPInfo\\PHPInfoServiceProvider"
            ]
        }
    }
}
